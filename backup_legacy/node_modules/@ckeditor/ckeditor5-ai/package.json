{
  "name": "@ckeditor/ckeditor5-ai",
  "version": "45.2.2",
  "description": "AI Assistant feature for CKEditor 5.",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "CKSource (http://cksource.com/)",
  "homepage": "https://ckeditor.com/ckeditor-5",
  "keywords": [
    "CKEditor",
    "ckeditor5",
    "ckeditor 5",
    "WYSIWYG",
    "WYSIWYW",
    "text",
    "rich-text",
    "richtext",
    "editor",
    "editing",
    "html",
    "operational transformation",
    "ot",
    "AI",
    "framework"
  ],
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "3.621.0",
    "@ckeditor/ckeditor5-clipboard": "45.2.2",
    "@ckeditor/ckeditor5-core": "45.2.2",
    "@ckeditor/ckeditor5-engine": "45.2.2",
    "@ckeditor/ckeditor5-icons": "45.2.2",
    "@ckeditor/ckeditor5-table": "45.2.2",
    "@ckeditor/ckeditor5-ui": "45.2.2",
    "@ckeditor/ckeditor5-utils": "45.2.2",
    "ckeditor5": "45.2.2",
    "es-toolkit": "1.32.0"
  },
  "type": "module",
  "main": "src/index.js",
  "files": [
    "dist",
    "lang",
    "src/**/*.js",
    "src/**/*.d.ts",
    "theme",
    "build",
    "ckeditor5-metadata.json",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://support.ckeditor.com/hc/en-us/requests/new"
  },
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./dist/*": {
      "types": "./src/index.d.ts",
      "import": "./dist/*",
      "default": "./dist/*"
    },
    "./src/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*",
      "default": "./src/*"
    },
    "./build/*": "./build/*",
    "./lang/*": "./lang/*",
    "./theme/*": "./theme/*",
    "./ckeditor5-metadata.json": "./ckeditor5-metadata.json",
    "./package.json": "./package.json"
  },
  "obfuscated": true
}
